<div id="commonBanner">
    <div class="fixed-bottom-banner" id="lineBanner" style="display:none;">
        <div class="banner-wrapper">
            <div class="section-inner">
                <div class="fixed-banner-slider" id="lineBannerItems"></div>
            </div>
        </div>
    </div>

    <template v-for="(obj, idx) in floatBanners">
        <div v-if="obj.display == true" class="layer-popup banner-image-popup open" id="imageBanner">
            <div class="popup-container">
                <div class="popup-content">
                    <div class="image">
                        <a :href="obj.linkUrl1"><img :src="fileBasePath + obj.fileSrc" alt=""></a>
                    </div>
                </div>
                <div class="popup-footer">
                    <div class="today-hidden">
                        <input type="checkbox" :id="'todayHide' + idx" name="" class="icon-check" @click="hideOneDayFloatBanner(obj.bannerId)">
                        <label :for="'todayHide' + idx">오늘 하루 보지않기</label>
                    </div>
                    <button type="button" class="btn-close popup-close" @click="closeFloatBanner(obj.bannerId)">닫기</button>
                </div>
            </div>
        </div>
    </template>
<div>

<script>
    let commonBanner = new Vue({
        el: '#commonBanner',
        data: {
            //orgId: '',
            orgId: '',
            lineBanners: [],
            floatBanners: [],
            cookiePrefix: 'BNR-',
            cookieValue: 'Y',
            fileBasePath: API_FILE_URL,
        },
        create() {
        },
        mounted() {
            this.init();

        },
        methods: {
            init: function() {
                var path ='/univ/program/allList';
                if(path == '/' || path.indexOf('/univ/main/main') > -1 || path.indexOf('/univ/local/view') > -1){
                    this.orgId = this.orgId.toUpperCase();
                    this.getBanners();
                }
            },

            getBanners: function(){
                apiCall('GET', '/banner/list', {orgId: this.orgId, userOrg: false, type: 2})
                .then((res) => {
                    var banners = res.data;

                    if(banners.length > 0){
                        for(var i=0; i<banners.length; i++){
                            var obj = banners[i];
                            obj.display = true;
                            var cookieValue = getCookie(this.cookiePrefix + obj.bannerId);
                            if(cookieValue != this.cookieValue){
                                if(obj.type == 2){
                                    // 하단 띠 배너
                                    this.lineBanners.push(obj);

                                }else if(obj.type == 1){
                                    // 이미지 팝업 배너
                                    this.floatBanners.push(obj);
                                }
                            }
                        }

                        this.makeLineBannerContents();
                    }
                })
                .catch((err) => {
                    console.log(err);
                    openSystemPopup(MESSAGE.E900);
                });
            },

            // 하단 띠배너 아이템 생성.
            makeLineBannerContents: function(){
                var items = '';
                for(var i=0; i<this.lineBanners.length; i++){
                    var obj = this.lineBanners[i];
                    items += '<div class="slider-item">';
                    if(obj.linkUrl1 != ''){
                        items +=  '    <span class="banner-text"><a href="' + obj.linkUrl1 + '">' + obj.title + '</a></span>';
                    }else{
                        items +=  '    <span class="banner-text">' + obj.title + '</span>';
                    }
                    items +=  '</div>';
                }
                if(items != ''){
                    window.sliderMaker();
                    $('.fixed-banner-slider').slick('slickAdd', items);
                    $("[id=lineBanner]").show();
                    window.fixedBottomBannerController();
                }else{
                    $("[id=lineBanner]").hide();
                }
                window.fixedBottomBannerController();
            },

            // 이미지 팝업 배너 아이템 생성.
            makeFloatBannerContents: function(){

            },

            // 오늘 하루 안보기
            hideOneDayFloatBanner: function(bannerId){
                this.closeFloatBanner(bannerId);
                setCookie(this.cookiePrefix + bannerId, this.cookieValue, 1);
            },

            // 배너 닫기
            closeFloatBanner: function(bannerId) {
                var obj = this.floatBanners.filter(obj => obj.bannerId == bannerId)[0];
                obj.display = false;
            },

        }
    })
</script>
		<!----------------------- banner ---------------------------->
	</div>
</body>
</html>







