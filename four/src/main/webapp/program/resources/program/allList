<script>
    let globalHeader = new Vue({
        el: '#globalHeader',
        data: {
            isLogin: false,                 // 로그인 여부
            user: {},                       // 로그인 사용자 정보
            fileBasePath: API_FILE_URL,     // 파일 prefix
        },
        created() {
        },
        mounted() {
            this.init();
        },
        methods: {
            init: function(){
                this.isLogin = global.getLogin();
                if(this.isLogin){
                    this.user = global.getLoginUser();
                }
            },
        },
    })
</script>
