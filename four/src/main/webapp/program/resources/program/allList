<script>
    const API_SERVER_URL = 'https://api.sangsanguniv.com/api/v1';
    const API_FILE_URL   = 'https://sjs888056-sjs888056.ktcdn.co.kr/';

    /***************************
	* 공통 API 호출.
	****************************/
	function apiCall(type, url, param, localhostCallYN) {
		var contentType = "text/plain;charset=UTF-8";
		
		if(type == "POST"){
			contentType = "application/json;charset=UTF-8";
			param = JSON.stringify (param);
		}
		
		if(localhostCallYN != 'Y'){
			url = 'https://api.sangsanguniv.com/api/v1' + url;
		}
		
	    return new Promise((resolve, reject) => {
			$.ajax({
		        url: url,
		        beforeSend: function (xhr) {
		            xhr.setRequestHeader('Authorization', 'Bearer ' + sessionStorage.getItem("UNIV_ACCESS_TOKEN"));
		        },
		    	type: type,
	        	data: param,
				contentType: contentType,
				fail: function (fail) {
		      		var strData = JSON.stringify(fail);
		      		strData = strData.replaceAll("\\r\\n","<br/>").replaceAll("\\n","<br/>").replaceAll("\\r","<br/>");
		      		fail = JSON.parse(strData);
					reject(fail);
				},
		      	success: function (data) {
		      		var strData = JSON.stringify(data);
		      		strData = strData.replaceAll("\\r\\n","<br/>").replaceAll("\\n","<br/>").replaceAll("\\r","<br/>");
		      		data = JSON.parse(strData);
		      		resolve(data);
		      	},
		      	error: function (error) {
		      		var strData = JSON.stringify(error);
		      		strData = strData.replaceAll("\\r\\n","<br/>").replaceAll("\\n","<br/>").replaceAll("\\r","<br/>");
		      		error = JSON.parse(strData);
		        	reject(error);
				}
		    });
		});
	}

	/***************************
    * 공통 API 호출.
    ****************************/
    function apiCallForSync(type, url, param, localhostCallYN) {
        var contentType = "text/plain;charset=UTF-8";

        if(type == "POST"){
            contentType = "application/json;charset=UTF-8";
            param = JSON.stringify (param);
        }

        if(localhostCallYN != 'Y'){
            url = 'https://api.sangsanguniv.com/api/v1' + url;
        }

        return new Promise((resolve, reject) => {
            $.ajax({
                url: url,
                beforeSend: function (xhr) {
                    xhr.setRequestHeader('Authorization', 'Bearer ' + sessionStorage.getItem("UNIV_ACCESS_TOKEN"));
                },
                type: type,
                data: param,
                async: false,
                contentType: contentType,
                fail: function (fail) {
                    var strData = JSON.stringify(fail);
                    strData = strData.replaceAll("\\r\\n","<br/>").replaceAll("\\n","<br/>").replaceAll("\\r","<br/>");
                    fail = JSON.parse(strData);
                    reject(fail);
                },
                success: function (data) {
                    var strData = JSON.stringify(data);
                    strData = strData.replaceAll("\\r\\n","<br/>").replaceAll("\\n","<br/>").replaceAll("\\r","<br/>");
                    data = JSON.parse(strData);
                    resolve(data);
                },
                error: function (error) {
                    var strData = JSON.stringify(error);
                    strData = strData.replaceAll("\\r\\n","<br/>").replaceAll("\\n","<br/>").replaceAll("\\r","<br/>");
                    error = JSON.parse(strData);
                    reject(error);
                }
            });
        });
    }
	
	/***************************
	* 파일 다운로드.
	****************************/	
	function getFileDown(fileId, fileName){
	    global.getLogin();

	    var fileUrl = "https://api.sangsanguniv.com/api/v1/files/" + fileId;

	    if (window.ReactNativeWebView) {
	        // 웹뷰일 경우.
            window.ReactNativeWebView.postMessage(
                JSON.stringify({ type: "download",  accessToken: sessionStorage.getItem("UNIV_ACCESS_TOKEN"), fileUrl: fileUrl})
            );
            return;
        }else{
            // 브라우저 일 경우.
            var xhr = new XMLHttpRequest();
            xhr.onreadystatechange = function(){
                if (this.readyState == 4){
                    if(this.status == 200){
                        var _data = this.response;
                        var _blob = new Blob([_data], {type:' application/octet-stream '});
                        var link = document.createElement('a');
                        link.href = window.URL.createObjectURL(_blob);
                        link.download = fileName;
                        link.click();
                    } else {
                        openSystemPopup("존재하지 않는 파일입니다.<br/>사이트 문의를 통해 관리자에게 문의하여주세요.");
                    }
                }
            }
            xhr.open('GET', fileUrl);
            xhr.setRequestHeader('Authorization', 'Bearer ' + sessionStorage.getItem("UNIV_ACCESS_TOKEN"));
            xhr.responseType = 'blob';
            xhr.send();
        }
	}
	
	/***************************
