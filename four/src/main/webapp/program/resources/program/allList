<script type="text/javascript">

    // 상상메이트
    var SANGSANG_MATE_EVENT_START_DATE = moment('2022-03-03 00:00:00');
    var SANGSANG_MATE_EVENT_END_DATE = moment('2022-03-31 23:59:59');

    // 출석체크
    var ATTENDANCE_CHECK_EVENT_START_DATE = moment('2022-03-03 00:00:00');
    var ATTENDANCE_CHECK_EVENT_END_DATE = moment('2022-03-31 23:59:59');

    // 상상 캠퍼스 포춘/타로 주제 투표
    var FORTUNE_EVENT_START_DATE = moment('2022-03-03 00:00:00');
    var FORTUNE_EVENT_END_DATE = moment('2022-03-31 23:59:59');

    // 상상캐치
    var SANGSANG_CATCH_EVENT_START_DATE = moment('2022-03-10 13:00:00');
    var SANGSANG_CATCH_EVENT_END_DATE = moment('2022-04-10 23:59:59');

    function isBeforeEvent(date) {
        return moment().isBefore(date);
    }

    function isAfterEvent(date) {
        return moment().isAfter(date);
    }

    var catchEventCharacterArr = ['S', 'A', 'N', 'G', '2'];
    $(document).ready(function () {
        initCatchEvent();
    });

    function getMyCatchList() {
        return apiCall('GET', '/event/getCatchList', {});
    }

    function initCatchEvent() {
        if (isBeforeEvent(SANGSANG_CATCH_EVENT_START_DATE)) {
            return false;
        }

        if (isAfterEvent(SANGSANG_CATCH_EVENT_END_DATE)) {
            return false;
        }

        catchEventCharacterArr = ['S', 'A', 'N', 'G', '2'];
        $('#catchCompleteImage').find('img').hide();

        var $container = $('#eventCatchFixedCharacter')
        $container.hide();
        $container.attr('data-character', '');
        $container.find('.image').hide();
        $container.find('.image>.img-on').css('opacity', 1);
        var url = window.location.pathname;
        if (!(url.indexOf('/univ/login/login') > -1 ||
            url.indexOf('/univ/signup/gate') > -1 ||
            url.indexOf('/univ/signup/process') > -1 ||
            url.indexOf('/univ/login/searchId') > -1 ||
            url.indexOf('/univ/login/searchPassword') > -1 ||
            url.indexOf('/univ/editInfo/login') > -1 ||
            url.indexOf('/univ/editInfo/edit') > -1 ||
            url.indexOf('/univ/editInfo/password') > -1
        )) {
            if (global.getLogin()) {
                getMyCatchList()
                    .then((res) => {
                        var myCatchArr = res.data;
                        for (var i = 0; i < myCatchArr.length; i++) {
                            var item = myCatchArr[i];
                            var index = catchEventCharacterArr.indexOf(item.catchCharacter);
                            if (index !== -1) {
                                catchEventCharacterArr.splice(index, 1);
                            }
                        }
                        showCatchEventCharacter();

                        if (url.indexOf('/event/sangsangCatch') > -1) {
                            try {
                                initMyCatchList()
                            } catch (e) {
                            }
                        }
                    })
                    .catch((err) => {
                        openSystemPopup(MESSAGE.E900);
                    });
            } else {
                showCatchEventCharacter();
            }
        }
    }

    function showCatchEventCharacter() {
        if (catchEventCharacterArr.length > 0) {
            var url = window.location.pathname;
            var character = '';
            if (url === '/univ/spaceAtelier/introduce' || url === '/univ/spaceAtelier/reservation') {
                character = 'S';
            } else if (url === '/univ/program/allList') {
                character = 'A';
            } else if (url === '/univ/local/list' || url === '/univ/local/view') {
                character = 'N';
            } else if (url === '/univ/programClass/list') {
                character = 'G';
            } else if (url === '/univ/programActivity/list') {
                character = '2';
            } else if (url === '/univ/program/view') {
                if (Math.random() < 0.5) {
                    character = 'G';
                } else {
                    character = '2';
                }
            }
            if (catchEventCharacterArr.indexOf(character) > -1) {
                var image = $('#eventCatchFixedCharacter .image[data-character="' + character + '"]');
                image.show();

                var container = $('#eventCatchFixedCharacter');
                var browserWidth = $(window).outerWidth();
                var browserHeight = $(window).outerHeight();
                var headerHeight = $('header#header').outerHeight();
                var bannerHeight = $('div#lineBanner').outerHeight();
                var fixedMobileBottomMenu = $('.fixed-mobile-bottom-menu').outerHeight();
                container.css('left', getRandom(0, browserWidth - image.outerWidth()));
                container.css('top', getRandom(headerHeight, browserHeight - (headerHeight + bannerHeight + fixedMobileBottomMenu)));
                container.show();
                container.data('character', character);
            }
        }

        function getRandom(min, max) {
            return Math.floor(Math.random() * (max - min)) + min;
        }
    }

    var submitCatchProcessing = false;

    function eventCatchClickAnimation(target) {
        if (submitCatchProcessing) {
            return false;
        }
        var $this = $(target);
        var $character = $('#eventCatchFixedCharacter');
        var catchCharacter = $character.data('character');
        submitCatchProcessing = true;
        if (global.getLogin()) {
            apiCall('POST', '/event/submitCatch', {catchCharacter: catchCharacter})
                .then((res) => {
                    if (res.success) {
                        $this.find('.img-on').animate({
                            opacity: 0.15
                        }, 1200, function () {
                            $('#catchCompleteImage').find('img[data-character="' + catchCharacter + '"]').show();
                            openLayerPopup('eventCatchCompletePopup');
                            $character.fadeOut(300);
                        });

                        if (window.location.pathname.indexOf('/event/sangsangCatch') > -1) {
                            try {
                                setTimeout(function () {
                                    initMyCatchList()
                                }, 1500)
                            } catch (e) {
                            }
                        }
                    } else {
                        if (isNull(res.data)) {
                            $('#alertMessage').html(res.message);
                            openLayerPopup('customEventAlertPopup');
                        } else {
                            switch (res.data) {
                                case 'BEFORE_PERIOD':
                                    openLayerPopup('beforeEventPeriodPopup');
                                    break;
                                case 'AFTER_PERIOD':
                                    openLayerPopup('afterEventPeriodPopup');
                                    break;
                                case 'NOT_LOGIN':
                                    openLayerPopup('notEventLoginPopup');
                                    break;
                                case 'ALREADY_CAUGHT_CHARACTER':
                                    openLayerPopup('alreadyCaughtCharacterPopup');
                                    break;
                                default :
                                    break;
                            }
                        }
                        $this.find('.img-on').animate({
                            opacity: 0.15
                        }, 1200, function () {
                            $character.fadeOut(300);
                        });
                    }
                    submitCatchProcessing = false;
                })
                .catch((err) => {
                    openSystemPopup(MESSAGE.E900);
                    submitCatchProcessing = false;
                });
        } else {
            openLayerPopup('eventCatchGuidePopup');
            submitCatchProcessing = false;
        }
    }

    $('#btnMoveToEventLogin').click(function () {
        var url = window.location.pathname;
        var queryParam = window.location.search;
        gotoLogin(url + queryParam);
    })

</script>
