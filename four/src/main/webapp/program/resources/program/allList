<script>
// infinite 스크롤 함수
var infiniteFlag = true;
$(function() {
	function infiniteScrollFn() {
		var $el = $('.infinite-scroll-el');
		if (!$el.length) return;
		var _sct = $(window).scrollTop();
		var _windowHeight = $(window).height();
		var _offsetTop = $el.offset().top;
		var _elHeight = $el.outerHeight();
		if (_sct + _windowHeight >= _offsetTop + _elHeight && infiniteFlag) {
		  _ss_programAllList.fnSrch();
		}
	}

	$(window).on('scroll', function() {
		infiniteScrollFn();
	});
});
</script>
		</main>
